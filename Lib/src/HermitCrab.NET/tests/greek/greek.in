;$Id$
;Classical (Koine?) Greek, as dumped by LL 13 April 1998
; for investigating cause of high memory usage associated with a large dictionary.
; For that reason, I've kept the Cellar HexIDs in place of the names.  The ff. is the xltn:
; 1EE009910DA9C54 = Lexical (stratum)

(open_language  Greek, Ancient)
;FEATURES
(morpher_set
 (*pfeatures*
  ('syllabic' ('+' '-' )
   'consonantal' ('+' '-' )
   'sonorant' ('+' '-' )
   'continuant' ('+' '-' )
   'nasal' ('+' '-' )
   'voiced' ('+' '-' )
   'aspirated' ('+' '-' )
   'coronal' ('+' '-' )
   'anterior' ('+' '-' )
   'high' ('+' '-' )
   'low' ('+' '-' )
   'back' ('+' '-' )
   'round' ('+' '-' )
   'strident' ('+' '-' )
   'distributed' ('+' '-' )
   'delayed release' ('+' '-' )
   'covered' ('+' '-' )
   'lateral' ('+' '-' )
   'tense' ('+' '-' )
   'stress' ('+' '-' )
   'long' ('+' '-' )
  )
 )
)
;CTABLE
(load_char_def_table
  <char_table name 'Lexical' encoding Greek, Ancient Phonemic
  seg_defs
	(
	  (a ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'continuant' '+' 'high' '-' 'low' '+' 'back' '+' 'round' '-' ))
	  (e ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'high' '-' 'low' '-' 'back' '-' 'round' '-' 'long' '-' ))
	  (j ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'high' '-' 'low' '-' 'back' '-' 'round' '-' 'long' '+' ))
	  (i ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'high' '-' 'low' '-' 'back' '-' 'round' '-' ))
	  (o ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'high' '-' 'low' '-' 'back' '+' 'round' '+' 'long' '-' ))
	  (w ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'high' '-' 'low' '-' 'back' '+' 'round' '+' 'long' '+' ))
	  (u ('syllabic' '+' 'consonantal' '-' 'sonorant' '+' 'high' '+' 'low' '-' 'back' '+' 'round' '+' 'long' '-' ))
	  (b ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '+' 'coronal' '-' 'anterior' '+' 'back' '-' ))
	  (d ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '+' 'coronal' '+' 'anterior' '+' 'back' '-' 'lateral' '-' ))
	  (g ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '+' 'coronal' '-' 'anterior' '-' 'back' '+' ))
	  (p ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '-' 'aspirated' '-' 'coronal' '-' 'anterior' '+' 'back' '-' ))
	  (t ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '-' 'aspirated' '-' 'coronal' '+' 'anterior' '+' 'back' '-' ))
	  (k ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '-' 'aspirated' '-' 'coronal' '-' 'anterior' '-' 'back' '+' ))
	  (f ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '-' 'aspirated' '+' 'coronal' '-' 'anterior' '+' 'back' '-' ))
	  (q ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '+' 'nasal' '-' 'voiced' '-' 'aspirated' '-' 'coronal' '+' 'anterior' '+' 'back' '-' 'strident' '-' ))
	  (c ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '-' 'nasal' '-' 'voiced' '-' 'aspirated' '+' 'coronal' '-' 'anterior' '-' 'back' '+' ))
	  (z ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '+' 'nasal' '-' 'voiced' '+' 'coronal' '+' 'anterior' '+' 'back' '-' 'strident' '+' ))
	  (< ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '+' 'voiced' '-' 'coronal' '-' 'anterior' '-' 'high' '-' 'back' '+' ))
	  (s ('syllabic' '-' 'consonantal' '+' 'sonorant' '-' 'continuant' '+' 'nasal' '-' 'voiced' '-' 'coronal' '+' 'anterior' '+' 'back' '-' 'strident' '+' ))
	  (m ('syllabic' '-' 'consonantal' '+' 'sonorant' '+' 'continuant' '-' 'nasal' '+' 'voiced' '+' 'coronal' '-' 'anterior' '+' 'back' '-' ))
	  (n ('syllabic' '-' 'consonantal' '+' 'sonorant' '+' 'continuant' '-' 'nasal' '+' 'voiced' '+' 'coronal' '+' 'anterior' '+' 'back' '-' 'lateral' '-' ))
	  (l ('syllabic' '-' 'consonantal' '+' 'sonorant' '+' 'continuant' '-' 'nasal' '-' 'voiced' '+' 'coronal' '+' 'anterior' '+' 'back' '-' 'lateral' '+' ))
	  (r ('syllabic' '-' 'consonantal' '+' 'sonorant' '+' 'continuant' '-' 'nasal' '-' 'voiced' '+' 'coronal' '+' 'anterior' '+' 'back' '-' 'lateral' '-' ))
	)
   >
)
;STRATA
(morpher_set (*strata* ('1EE009910DA9C54' )))  ;Lexical
(set_stratum <stratum_setting nm '1EE009910DA9C54' type ctable value 'Lexical'>)
(set_stratum <stratum_setting nm '1EE009910DA9C54' type cyclicity value noncyclic>)
(set_stratum <stratum_setting nm '1EE009910DA9C54' type prule value linear >)
(set_stratum <stratum_setting nm '1EE009910DA9C54'  type templates value (  )>)

;Don't let errors stop us:
(morpher_set message (*quit_on_error* false))


(merge_in_dictionary_file message c:\data\hc\morpher\tests\greek\greek.dic)
