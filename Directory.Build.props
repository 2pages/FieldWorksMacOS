<Project>
	<PropertyGroup>
		<!-- Switched to net472 instead of net462 - 4.6.x is missing /usr/lib/mono/4.6*/Facades/netstandard.dll which causes build failures -->
		<TargetFrameworks>net472</TargetFrameworks>
		<RuntimeIdentifier>win10-x64</RuntimeIdentifier>
		<Configurations>Debug;Release</Configurations>
		<Description>FieldWorks</Description>
		<Company>SIL International</Company>
		<Authors>SIL International</Authors>
		<Product>FieldWorks</Product>
		<Copyright>Copyright Â© 2002-2023 SIL International</Copyright>
		<OutputPath>../../Output/$(Configuration)</OutputPath>
		<TargetDir>$(OutputPath)</TargetDir>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<LangVersion>8</LangVersion>
		<AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
		<GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
	</PropertyGroup>
</Project>