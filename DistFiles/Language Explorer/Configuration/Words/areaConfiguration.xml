				</parameters>
			</guicontrol>
			<guicontrol id="GlossConcordanceBrowseView">
				<parameters id="GlossInTwficsOccurrenceList" selectColumn="true" defaultChecked="true" omitFromHistory="true" allowInsertDeleteRecord="false" editable="false" clerk="twficOccurrencesOfWfiGloss" filterBar="true" ShowOwnerShortname="true">
