		<tool label="Feature Types" value="featureTypesAdvancedEdit" icon="SideBySideView">
			<control>
				<dynamicloaderinfo assemblyPath="xCore.dll" class="XCore.MultiPane"/>
				<parameters area="lists" id="FeatureTypesAndDetailMultiPane" splitterBarOrientation="vertical" firstLabel="Browse" secondLabel="Details">
					<!--this will fill the pane on the left-->
					<control>
						<dynamicloaderinfo assemblyPath="xCore.dll" class="XCore.PaneBarContainer"/>
						<parameters id="featureTypesList" collapse="144000">
							<control>
								<dynamicloaderinfo assemblyPath="xWorks.dll" class="SIL.FieldWorks.XWorks.RecordBrowseView"/>
								<parameters area="lists" id="featureTypesList" clerk="featureTypes" filterBar="true" treeBarAvailability="NotAllowed"
											defaultCursor="Arrow" hscroll="true" altTitleId="FsFeatStrucType-Plural">
									<columns>
										<column label="Name" ws="$ws=best analysis" width="50%" field="Name">
											<span>
												<properties>
													<editable value="false"/>
												</properties>
												<string field="Name" ws="best analysis"/>
											</span>
										</column>
										<column label="Abbreviation" width="50%" ws="$ws=best analysis" field="Abbreviation">
											<span>
												<properties>
													<editable value="false"/>
												</properties>
												<string field="Abbreviation" ws="best analysis"/>
											</span>
										</column>
									</columns>
								</parameters>
							</control>
						</parameters>
					</control>
					<!--this will fill the pane on the right-->
					<control>
						<dynamicloaderinfo assemblyPath="xCore.dll" class="XCore.PaneBarContainer"/>
						<parameters PaneBarGroupId="PaneBar-ShowHiddenFields-featureTypesAdvancedEdit" collapse="144000">
							<control>
								<dynamicloaderinfo assemblyPath="xWorks.dll" class="SIL.FieldWorks.XWorks.RecordEditView"/>
								<parameters area="lists" clerk="featureTypes" treeBarAvailability="NotAllowed" filterPath="Language Explorer\Configuration\Grammar\Edit\DataEntryFilters\basicPlusFilter.xml" persistContext="advancedEdit" layout="Edit"/>
							</control>
						</parameters>
