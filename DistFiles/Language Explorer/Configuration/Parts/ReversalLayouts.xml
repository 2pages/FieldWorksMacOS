<?xml version="1.0" encoding="UTF-8"?>
<!-- Hand crafted layouts for Reversal Index related classes (or layouts). -->
<LayoutInventory>
	<layout class="ReversalIndexEntry" type="detail" name="Normal">
		<part ref="ChangeHandler"/>
		<part ref="ReversalForm" label="Form" menu="mnuDataTree-MoveReversalIndexEntry"/>
		<part ref="PartOfSpeech"/>
		<part ref="CurrentSenses"/>
		<part ref="Section" expansion="expanded" label="Subentries" menu="mnuDataTree-InsertReversalSubentry" hotlinks="mnuDataTree-InsertReversalSubentry-Hotlinks">
			<indent indent="true">
				<part ref="ReversalSubentries" param="Subentry"/>
			</indent>
		</part>
	</layout>
	<layout class="ReversalIndexEntry" type="detail" name="Subentry">
		<part ref="ReversalForm" label="Form" menu="mnuDataTree-MoveReversalIndexEntry"/>
