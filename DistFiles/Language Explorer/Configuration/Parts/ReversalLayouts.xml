			</part>
		</generate>
		<generate class="MoForm" fieldType="gendate" restrictions="customOnly">
			<part ref="$child" label="$label" before="" after=" " visibility="never">
				<gendate field="$fieldName" format="short"/>
			</part>
		</generate>
		<generate class="MoForm" fieldType="owningatom" restrictions="customOnly">
			<part ref="$child" label="$label" before="" after=" " visibility="never">
				<obj field="$fieldName" layout="FullTextFakePara"/>
			</part>
		</generate>
	</layout>

