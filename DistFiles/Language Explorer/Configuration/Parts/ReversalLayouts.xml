			</indent>
		</part>
	</layout>

	<layout class="ReversalIndexEntry" type="jtview" name="publishReversal" tagForWs="true">
		<part ref="ReversalConfig" param="publishReversalIndexEntry"/>
	</layout>

	<layout class="ReversalIndexEntry" type="jtview" name="publishReversalIndexEntry" tagForWs="true">
