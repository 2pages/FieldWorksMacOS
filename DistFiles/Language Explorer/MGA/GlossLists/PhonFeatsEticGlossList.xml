					 <f name='fPAConstrictedGlottis'>
						 <sym value='+'/>
					 </f>
				 </fs>
			 </item>
			 <item id='vPAConstrictedGlottisNegative' guid="ca0fbc96-5d68-4e26-a48d-29dc3cd2d207" type='value'>
