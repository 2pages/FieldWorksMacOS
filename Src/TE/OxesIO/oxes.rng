	<define name="headerCT">
		<jaa:tag type="block">
			<jaa:tagName>header</jaa:tagName>
			<jaa:p type="1"><tagName idref="header"/> identifies metadata (data about data) for this
				Bible or New Testament.</jaa:p>
			<jaa:p>It contains <tagName idref="revisionDesc"/>, <tagName idref="work"/>, <tagName
					idref="title"/>, <tagName idref="contributor"/></jaa:p>
