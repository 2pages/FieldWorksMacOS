	<define name="wordsOfJesusCT">
		<jaa:tag id="Words_Of_Christ" picture="NIV MRK 1.41-42" picture2="NIV REV 1.9-12"
			picture3="NIV JHN 11.43-44" type="inline">
			<jaa:tagName>wordsOfJesus</jaa:tagName>
			<jaa:p type="1"><tagName idref="wordsOfJesus"/> identifies words or phrases that are
				spoken by Jesus. </jaa:p>
			<jaa:p>They are found in <bookTitle>Matthew, Mark, Luke, John, Acts, 1 Corinthians, 2
					Corinthians, Hebrews, 2 John, and Revelation.</bookTitle> These are sometimes
				formatted in red.</jaa:p>
			<jaa:usfm>n/a</jaa:usfm>
			<jaa:TE>Words Of Jesus</jaa:TE>
			<jaa:osis>q who="Jesus"</jaa:osis>
		</jaa:tag>
