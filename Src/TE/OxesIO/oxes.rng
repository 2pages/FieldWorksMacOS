	<define name="speakerCT">
		<jaa:tag id="Speech_Speaker" context="text" picture="TEV SNG 2.1-3" type="block">
			<jaa:tagName>speaker</jaa:tagName>
			<jaa:p type="1"><tagName idref="speaker"/> identifies the speaker, usually in drama as
				in Job and Song of Solomon. This is not part of the original text. It is shown in a
				heading.</jaa:p>
			<jaa:p><example>&lt;l type="attribution"&gt;</example> identifies the speaker
				(usually the Lord) when it is in the original text. </jaa:p>
			<jaa:p>See also: <tagName idref="speech"/>, <tagName idref="l"/></jaa:p>
			<jaa:usfm>sp</jaa:usfm>
			<jaa:TE>Speech Speaker</jaa:TE>
			<jaa:osis>speaker</jaa:osis>
		</jaa:tag>
		<mixed>
			<group>
				<ref name="globalWithType"/>
				<optional>
					<attribute name="who">
						<data type="string"/>
					</attribute>
				</optional>
				<optional>
					<attribute name="canonical">
						<data type="boolean"/>
					</attribute>
				</optional>
			</group>
			<zeroOrMore>
				<choice>
