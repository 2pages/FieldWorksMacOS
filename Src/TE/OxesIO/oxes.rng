				&lt;div[@type="index" @subtype="x-names"]&gt;, &lt;div[@type="index"
				@subtype="x-topical"]&gt;, &lt;div[@type="map"]&gt;,
				&lt;div[@type="x-weightsAndMeasures"]</jaa:osis>
		</jaa:tag>
		<optional>
			<attribute name="canonical">
				<data type="boolean"/>
			</attribute>
		</optional>

		<optional>
			<choice>
				<element name="titlePage">
					<ref name="titlePageCT"/>
				</element>
			</choice>
		</optional>
		<oneOrMore>
			<choice>
				<element name="glossary">
					<ref name="glossaryCT"/>
				</element>
				<element name="section">
					<ref name="sectionCT"/>
				</element>
			</choice>
		</oneOrMore>
	</define>

