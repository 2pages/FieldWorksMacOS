	<define name="suppliedCT">
		<jaa:tag id="Supplied" picture="Suggested Source Translation (SST) of MARK by Ellis Deibler"
			publish="yes" type="inline">
			<jaa:tagName>supplied</jaa:tagName>
			<jaa:p type="1"><tagName idref="supplied"/> identifies words that are not literally in
				the original text, but that are supplied to make the original meaning clear. This is
				usually called <ew>implicit information</ew> by translators.</jaa:p>
			<jaa:usfm>add ... add*</jaa:usfm>
			<jaa:TE>Supplied</jaa:TE>
			<jaa:osis>transChange type="added"</jaa:osis>
		</jaa:tag>
