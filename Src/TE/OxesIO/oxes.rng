	<define name="languageCT">
		<jaa:tag type="block">
			<jaa:tagName>language</jaa:tagName>
			<jaa:p type="1">The <tagName idref="language"/> identifies the language used in the
				document. This is for the body of the text. The back translation language it set on
				the <tagName idref="bt"/> element.</jaa:p>
			<jaa:p>ISO 639-3 is assumed for the language code. This is the current <abbreviation
					expansion="SIL International - formerly known as Summer Institute of Linguistics"
					>SIL</abbreviation> Ethnologue code also. http://www.sil.org/iso639-3/</jaa:p>
		</jaa:tag>
		<group>
			<data type="string"/>
			<optional>
				<attribute name="type">
					<data type="string">
						<param name="pattern">[a-z]{3}</param>
						<!-- 3 lower case letters required to match sil standard -->
					</data>
				</attribute>
			</optional>
			<optional>
				<attribute name="use">
					<choice>
						<choice>
							<value type="string">interlinear</value>
							<value type="string">translation</value>
							<value type="string">source</value>
							<value type="string">target</value>
							<value type="string">original</value>
							<value type="string">base</value>
							<value type="string">didactic</value>
							<value type="string">quotation</value>
						</choice>
						<data type="string">
							<param name="pattern">([^\s])+</param>
						</data>
					</choice>
				</attribute>
			</optional>
			<ref name="globalWithoutType"/>
			<optional>
				<attribute name="canonical">
					<data type="boolean"/>
				</attribute>
			</optional>
		</group>
	</define>

	<define name="embeddedCT">
		<jaa:tag id="embedded" type="block" picture="NIV ACT 15.23-31 [containing several p]"
			picture2="NLT REV 22.15-18 [containing p]"
			picture3="NLT 1TI 3.16 [containing several l]">
			<jaa:tagName>embedded</jaa:tagName>
			<jaa:p type="1"><tagName idref="embedded"/> identifies text that is embedded in the main
				text.</jaa:p>
			<jaa:p> Possible types of embedded texts include: </jaa:p>
			<jaa:list>
				<jaa:item>Lettters (found in 1 Kings, Ezra, Nehemiah, Jeremiah, Acts, Revelation,
					...),</jaa:item>
				<jaa:item>Messages (found in Numbers 20, 2 Kings 18, ...),</jaa:item>
				<jaa:item>Prayers (found in John 17, Daniel 9, ...),</jaa:item>
				<jaa:item>Citations within letter (found in Revelation 2.27, ...),</jaa:item>
				<jaa:item>Songs (found in Philippians 2, ...),</jaa:item>
				<jaa:item>Announcements (found in Daniel 4, Revelation 22, ...),</jaa:item>
				<jaa:item>Proclamations (found in 2 Chronicles 36, Ezra 1, ...), and</jaa:item>
				<jaa:item>Decrees (found in Daniel 6, Jonah 3, ...)</jaa:item>
			</jaa:list>
			<jaa:p><example>Example 1</example> shows embedded text. The darker shading indicates a
					<tagName idref="p"/> within the <tagName idref="embedded"/> text. Above the
					<tagName idref="p"/> are three <example>&lt;p type="opening"/></example> and
				below is <example>&lt;p type="closing"/></example>.</jaa:p>

			<jaa:usfm>pmc, qm, qm1, qm2, qm3, pmo, pm, mi, pmr</jaa:usfm>
			<jaa:TE>Embedded Text Closing, Embedded Text Line1, Embedded Text Line2, Embedded Text
				Line3, Embedded Text Opening, Embedded Text Paragraph, Embedded Text Paragraph
				Continuation, Embedded Text Refrain</jaa:TE>
			<jaa:osis>q type="embedded"</jaa:osis>
		</jaa:tag>

		<oneOrMore>
			<choice>
				<element name="p">
					<ref name="pCT"/>
				</element>
				<element name="l">
					<ref name="lCT"/>
				</element>
				<element name="stanzaBreak">
					<ref name="stanzaBreakCT"/>
				</element>
			</choice>
		</oneOrMore>
	</define>

	<define name="alternateReadingCT">
		<jaa:tag id="Alternate_Reading" picture="TEV MAT 6.11 note" picture2="NLT MRK 1.7 note"
			publish="yes" type="inline">
			<jaa:tagName>alternateReading</jaa:tagName>
			<jaa:p type="1"><tagName idref="alternateReading"/> identifies an alternate translation
				for a passage. That is, the passage could be translated in this alternate way. It is
				a reading of approximately equal probability compared to others. It is used in
					<tagName idref="note"/>s.</jaa:p>
			<jaa:p><tagName idref="variant"/> identifies an alternate source text, not an alternate
				translation. It is
				a reading that varies from the accepted tradition.</jaa:p>
			<jaa:usfm>fqa, fq</jaa:usfm>
			<jaa:TE>Alternate Reading</jaa:TE>
			<jaa:osis>note&gt; ... &lt;rdg type="alternate"&gt; ... &lt;/rdg&gt;
				... &lt;note</jaa:osis>
		</jaa:tag>
		<ref name="grp.characterText"/>
	</define>


