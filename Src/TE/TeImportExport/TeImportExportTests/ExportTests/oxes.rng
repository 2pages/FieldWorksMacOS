	<define name="coverageCT">
		<group>
			<data type="string"/>
			<ref name="globalWithType"/>
			<optional>
				<attribute name="canonical">
					<data type="boolean"/>
				</attribute>
			</optional>
		</group>
	</define>

	<define name="oxesScripts">
		<data type="string">
			<param name="pattern">([A-Z][a-z]{3}|[A-Za-z0-9]+)</param>
		</data>
	</define>
	<define name="element0">
		<element>
			<anyName/>
			<mixed>
				<zeroOrMore>
					<choice>
						<ref name="element0"/>
						<attribute>
							<anyName/>
						</attribute>
					</choice>
				</zeroOrMore>
			</mixed>
		</element>
	</define>
	<define name="creatorCT">
		<group>
			<element name="creator">
				<data type="string"/>
				<!-- role is required not optional -->
				<attribute name="role">
					<ref name="roleType"/>
				</attribute>
				<optional>
					<attribute name="lang" ns="http://www.w3.org/XML/1998/namespace"/>
				</optional>
				<attribute name="ID">
					<!-- required ID and must be unique -->
				</attribute>
			</element>
		</group>
	</define>


