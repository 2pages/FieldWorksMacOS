	<define name="languageCT">
		<group>
			<data type="string"/>
			<optional>
				<attribute name="type">
					<data type="string">
						<param name="pattern">[a-z]{3}</param>
						<!-- 3 lower case letters required to match sil standard -->
					</data>
				</attribute>
			</optional>
			<optional>
				<attribute name="use">
					<choice>
						<choice>
							<value type="string">interlinear</value>
							<value type="string">translation</value>
							<value type="string">source</value>
							<value type="string">target</value>
							<value type="string">original</value>
							<value type="string">base</value>
							<value type="string">didactic</value>
							<value type="string">quotation</value>
						</choice>
						<data type="string">
							<param name="pattern">([^\s])+</param>
						</data>
					</choice>
				</attribute>
			</optional>
			<ref name="globalWithoutType"/>
			<optional>
				<attribute name="canonical">
					<data type="boolean"/>
				</attribute>
			</optional>
		</group>
	</define>
	<define name="embeddedCT">
