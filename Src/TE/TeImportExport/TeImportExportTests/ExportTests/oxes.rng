					<ref name="referenceCT"/>
				</element>
				<element name="seeInGlossary">
					<ref name="seeInGlossaryCT"/>
				</element>
				<element name="soCalled">
					<ref name="characterTextCT"/>
				</element>
				<element name="supplied">
					<ref name="characterTextCT"/>
				</element>
				<element name="title">
					<ref name="titleCT"/>
				</element>
				<element name="untranslatedWord">
					<ref name="untranslatedWordCT"/>
				</element>
				<element name="userCS">
					<ref name="userCSCT"/>
				</element>
				<element name="variant">
					<ref name="variantCT"/>
				</element>
				<element name="wordsOfJesus">
					<ref name="wordsOfJesusCT"/>
				</element>
			</choice>
		</zeroOrMore>
	</define>

<define name="userCSCT">
	<!-- user defined style -->
	<attribute name="type">
		<data type="string">
			<param name="pattern">([^\s])+</param>
		</data>
	</attribute>
</define>
	<!-- group empty -->
	<define name="grp.empty">
		<choice>
			<element name="chapter">
				<ref name="chapterCT"/>
			</element>
			<element name="verse">
				<ref name="verseCT"/>
			</element>
		</choice>
	</define>

	<!-- group notes -->
	<define name="grp.notes">
		<choice>
			<element name="note">
				<ref name="noteCT"/>
			</element>
			<element name="comment">
				<text/>
			</element>
		</choice>
	</define>

	<!-- group inline -->
	<define name="grp.inline">
		<zeroOrMore>
			<choice>
				<ref name="grp.notes"/>
				<ref name="grp.phrase"/>
				<element name="chapter">
					<ref name="chapterCT"/>
				</element>
				<element name="verse">
					<ref name="verseCT"/>
				</element>
			</choice>
		</zeroOrMore>
	</define>

	<!-- group title -->
	<define name="grp.title">
		<!-- @short holds the running header -->
		<optional>
			<attribute name="short"/>
		</optional>
		<oneOrMore>
			<element name="title">
				<attribute name="type">
					<choice>
						<value>main</value>
						<value>secondary</value>
						<value>tertiary</value>
					</choice>
				</attribute>
				<ref name="tr-bt-textCT"/>
			</element>
		</oneOrMore>
	</define>

