				</attribute>
			</optional>
			<optional>
				<attribute name="subType">
					<ref name="attributeExtension"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="type">
					<ref name="attributeExtension"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="resp">
					<data type="string"/>
				</attribute>
			</optional>
			<optional>
