	<define name="asMilestones">
		<group>
			<optional>
				<attribute name="n">
					<data type="string"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="oxesID">
					<ref name="oxesIDType"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="ID"> </attribute>
			</optional>
			<ref name="sIDeID"/>
		</group>
		<optional>
			<attribute name="annotateRef">
				<ref name="annotateRefType"/>
			</attribute>
		</optional>
	</define>

	<define name="globalWithoutType">
		<group>
			<optional>
				<attribute name="script">
					<ref name="oxesScripts"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="space" ns="http://www.w3.org/XML/1998/namespace"/>
			</optional>
			<optional>
				<attribute name="lang" ns="http://www.w3.org/XML/1998/namespace"/>
			</optional>
			<optional>
				<attribute name="n">
					<data type="string"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="subType">
					<ref name="attributeExtension"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="resp">
					<data type="string"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="oxesID">
					<ref name="oxesIDType"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="ID"> </attribute>
			</optional>
			<optional>
				<attribute name="editions">
					<list>
						<zeroOrMore>
							<data type="NMTOKEN"/>
						</zeroOrMore>
					</list>
				</attribute>
			</optional>
			<optional>
				<attribute name="annotateType">
					<ref name="annotationType"/>
				</attribute>
			</optional>
			<optional>
				<attribute name="annotateWork">
					<data type="string"/>
				</attribute>
			</optional>
		</group>
		<optional>
			<attribute name="annotateRef">
				<ref name="annotateRefType"/>
			</attribute>
		</optional>
	</define>

	<define name="globalTrBt">
		<optional>
			<attribute name="space" ns="http://www.w3.org/XML/1998/namespace"/>
		</optional>
		<optional>
			<attribute name="lang" ns="http://www.w3.org/XML/1998/namespace"/>
		</optional>
		<optional>
			<attribute name="resp">
				<data type="string"/>
			</attribute>
		</optional>
		<ref name="tr-bt-textCT"/>
	</define>

	<define name="oxesRefType">
		<list>
			<zeroOrMore>
				<data type="string">
					<param name="pattern"
						>(((\p{L}|\p{N}|_)+)((\.(\p{L}|\p{N}|_)+)*)?:)?((\p{L}|\p{N}|_|(\\[^\s]))+)(\.(\p{L}|\p{N}|_|(\\[^\s]))*)*(!((\p{L}|\p{N}|_|(\\[^\s]))+)((\.(\p{L}|\p{N}|_|(\\[^\s]))+)*)?)?(@(cp\[(\p{Nd})*\]|s\[(\p{L}|\p{N})+\](\[(\p{N})+\])?))?(\-((((\p{L}|\p{N}|_|(\\[^\s]))+)(\.(\p{L}|\p{N}|_|(\\[^\s]))*)*)+)(!((\p{L}|\p{N}|_|(\\[^\s]))+)((\.(\p{L}|\p{N}|_|(\\[^\s]))+)*)?)?(@(cp\[(\p{Nd})*\]|s\[(\p{L}|\p{N})+\](\[(\p{N})+\])?))?)?</param>
				</data>
			</zeroOrMore>
		</list>
	</define>

	<define name="chapterCT">
		<ref name="asMilestones"/>
	</define>

	<define name="identifierCT">
		<group>
			<data type="string"/>
			<ref name="globalWithoutType"/>
			<optional>
				<attribute name="type">
					<choice>
						<value type="string">URN</value>
						<value type="string">ISSN</value>
						<value type="string">ISBN</value>
						<value type="string">SICI</value>
						<value type="string">OSIS</value>
						<value type="string">URL</value>
						<value type="string">LCCN</value>
						<value type="string">DOI</value>
						<value type="string">Dewey</value>
						<value type="string">URI</value>
					</choice>
				</attribute>
			</optional>
			<optional>
				<attribute name="canonical">
					<data type="boolean"/>
				</attribute>
			</optional>
		</group>
	</define>

