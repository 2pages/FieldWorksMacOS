			<group>
				<ref name="sIDeID"/>
				<zeroOrMore>
					<choice>
						<choice>
							<element name="a">
								<ref name="aCT"/>
							</element>
							<element name="abbreviation">
								<ref name="abbreviationCT"/>
							</element>
							<element name="date">
								<ref name="dateCT"/>
							</element>
							<element name="nameOfGod">
								<ref name="divineNameCT"/>
							</element>
							<element name="foreign">
								<ref name="foreignCT"/>
							</element>
							<element name="index">
								<ref name="indexCT"/>
							</element>
							<element name="lg">
								<ref name="lgCT"/>
							</element>
							<element name="list">
								<ref name="listCT"/>
							</element>
							<element name="mentioned">
								<ref name="mentionedCT"/>
							</element>
							<element name="note">
								<ref name="noteCT"/>
							</element>
							<element name="p">
								<ref name="pCT"/>
							</element>
							<element name="q">
								<ref name="qCT"/>
							</element>
							<element name="reference">
								<ref name="referenceCT"/>
							</element>
							<element name="speaker">
								<ref name="speakerCT"/>
							</element>
							<element name="verse">
								<ref name="verseCT"/>
							</element>
						</choice>
					</choice>
				</zeroOrMore>
				<optional>
					<attribute name="canonical">
						<data type="boolean"/>
					</attribute>
				</optional>
				<!--        <ref name="globalWithType"/>-->
			</group>
			<optional>
				<attribute name="TEIform">
					<value>salute</value>
				</attribute>
			</optional>
		</mixed>
	</define>

	<define name="sIDeID">
		<optional>
			<attribute name="eID">
				<data type="string"/>
			</attribute>
		</optional>
		<optional>
			<attribute name="sID">
				<data type="string"/>
			</attribute>
		</optional>
	</define>

	<define name="oxesIDType">
		<list>
			<zeroOrMore>
				<data type="string">
					<param name="pattern"
						>(((\p{L}|\p{N}|_)+)((\.(\p{L}|\p{N}|_)+)*)?:)?((\p{L}|\p{N}|_|(\\[^\s]))+)((\.(\p{L}|\p{N}|_|(\\[^\s]))+)*)?(!((\p{L}|\p{N}|_|(\\[^\s]))+)((\.(\p{L}|\p{N}|_|(\\[^\s]))+)*)?)?</param>
				</data>
			</zeroOrMore>
		</list>
	</define>

	<define name="speakerCT">
		<mixed>
			<group>
				<optional>
					<attribute name="TEIform">
						<value>sp</value>
					</attribute>
				</optional>
				<ref name="globalWithType"/>
				<optional>
					<attribute name="who">
						<data type="string"/>
					</attribute>
				</optional>
				<optional>
					<attribute name="canonical">
						<data type="boolean"/>
					</attribute>
				</optional>
			</group>
			<zeroOrMore>
				<choice>
					<choice>
						<element name="a">
							<ref name="aCT"/>
						</element>
						<element name="abbreviation">
							<ref name="abbreviationCT"/>
						</element>
						<element name="nameOfGod">
							<ref name="divineNameCT"/>
						</element>
						<element name="foreign">
							<ref name="foreignCT"/>
						</element>
						<element name="index">
							<ref name="indexCT"/>
						</element>
						<element name="note">
							<ref name="noteCT"/>
						</element>
						<element name="reference">
							<ref name="referenceCT"/>
						</element>
					</choice>

				</choice>
			</zeroOrMore>
		</mixed>
	</define>

	<define name="globalCreator">
