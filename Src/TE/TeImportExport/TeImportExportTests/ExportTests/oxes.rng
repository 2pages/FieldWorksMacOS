		  <ref name="globalWithType"/>
			<optional>
				<attribute name="canonical">
					<data type="boolean"/>
				</attribute>
			</optional>
		</group>
	</define>
	  -->
	<define name="oxesGenType">
		<list>
			<zeroOrMore>
				<data type="string">
					<param name="pattern"
					>((((\p{L}|\p{N}|_)+)(\.(\p{L}|\p{N}|_))*:)?([^:\s])+)</param>
				</data>
			</zeroOrMore>
		</list>
	</define>
	<define name="workCT">
		<attribute name="oxesWork">
			<ref name="oxesWorkType"/>
		</attribute>
		<element name="title">
			<ref name="titleCT"/>
		</element>
		<oneOrMore>
			<ref name="creatorCT"/>
		</oneOrMore>
	</define>




	<define name="noteCT">
		<mixed>
			<group>
				<optional>
					<attribute name="TEIform">
						<value>note</value>
					</attribute>
				</optional>
				<ref name="globalWithoutType"/>
				<ref name="unpublishedNotesCT"/>
				<optional>
					<attribute name="type">
						<choice>
							<value type="string">general</value>
							<value type="string">citation</value>
							<value type="string">exegesis</value>
							<value type="string">translation</value>
							<value type="string">speaker</value>
							<value type="string">explanation</value>
							<value type="string">alternative</value>
							<value type="string">study</value>
							<value type="string">allusion</value>
							<value type="string">variant</value>
							<value type="string">background</value>
							<value type="string">liturgical</value>
