			<Dialog Id="Start_Installation_Dialog" Width="375" Height="270" Title="[ProductName] [DialogSuffix]" TrackDiskSpace="yes" CustomPalette="yes">
				<Control Id="InstallNow" Type="PushButton" X="244" Y="244" Width="56" Height="17" Default="yes">
					<Text><![CDATA[[DialogFontDefault]&Install]]></Text>
					<Publish Event="SpawnWaitDialog" Value="WaitForCosting"><![CDATA[CostingComplete = 1]]></Publish>
					<Publish Event="SpawnDialog" Value="OutOfDisk"><![CDATA[OutOfDiskSpace = 1]]></Publish>
					<!-- The following lines (2 per language) make sure that the localized TE files will be
					selected if the user selects the main language pack for a given language. -->
					<Publish Event="AddLocal" Value="Chinese_TE"><![CDATA[((&Chinese = 3 AND !TE = 3) OR (!Chinese = 3 AND &TE = 3) OR (&Chinese = 3 AND &TE = 3)) AND NOT (&Chinese = 2)]]></Publish>
					<Publish Event="Remove" Value="Chinese_TE"><![CDATA[&Chinese = 2 OR &TE = 2]]></Publish>
					<Publish Event="AddLocal" Value="French_TE"><![CDATA[((&French = 3 AND !TE = 3) OR (!French = 3 AND &TE = 3) OR (&French = 3 AND &TE = 3)) AND NOT (&French = 2)]]></Publish>
					<Publish Event="Remove" Value="French_TE"><![CDATA[&French = 2 OR &TE = 2]]></Publish>
					<Publish Event="AddLocal" Value="Indonesian_TE"><![CDATA[((&Indonesian = 3 AND !TE = 3) OR (!Indonesian = 3 AND &TE = 3) OR (&Indonesian = 3 AND &TE = 3)) AND NOT (&Indonesian = 2)]]></Publish>
					<Publish Event="Remove" Value="Indonesian_TE"><![CDATA[&Indonesian = 2 OR &TE = 2]]></Publish>
					<Publish Event="AddLocal" Value="Korean_TE"><![CDATA[((&Korean = 3 AND !TE = 3) OR (!Korean = 3 AND &TE = 3) OR (&Korean = 3 AND &TE = 3)) AND NOT (&Korean = 2)]]></Publish>
					<Publish Event="Remove" Value="Korean_TE"><![CDATA[&Korean = 2 OR &TE = 2]]></Publish>
					<Publish Event="AddLocal" Value="Portuguese_TE"><![CDATA[((&Portuguese = 3 AND !TE = 3) OR (!Portuguese = 3 AND &TE = 3) OR (&Portuguese = 3 AND &TE = 3)) AND NOT (&Portuguese = 2)]]></Publish>
					<Publish Event="Remove" Value="Portuguese_TE"><![CDATA[&Portuguese = 2 OR &TE = 2]]></Publish>
					<Publish Event="AddLocal" Value="Russian_TE"><![CDATA[((&Russian = 3 AND !TE = 3) OR (!Russian = 3 AND &TE = 3) OR (&Russian = 3 AND &TE = 3)) AND NOT (&Russian = 2)]]></Publish>
					<Publish Event="Remove" Value="Russian_TE"><![CDATA[&Russian = 2 OR &TE = 2]]></Publish>
					<Publish Event="AddLocal" Value="Spanish_TE"><![CDATA[((&Spanish = 3 AND !TE = 3) OR (!Spanish = 3 AND &TE = 3) OR (&Spanish = 3 AND &TE = 3)) AND NOT (&Spanish = 2)]]></Publish>
					<Publish Event="Remove" Value="Spanish_TE"><![CDATA[&Spanish = 2 OR &TE = 2]]></Publish>
