	<ItemGroup>
		<IcuDataZipFiles Include="$(fwrt)/DistFiles/Icu$(IcuVersion)/UnicodeDataOverrides.txt" />
		<IcuDataZipFiles Include="$(fwrt)/DistFiles/Icu$(IcuVersion)/icudt$(IcuVersion)l/nfc_fw.nrm" />
