<open-project role="dialog" title="Open Project" path="18:Open Project/" root="yes">

  <look-in role="text" name="Look in:" path="9:$name;">
	<label name="Look in:" path="/41:$name;"/>
	<list path="[2]/36:#ANY" />
  </look-in>

  <choose role="text" name="Chose a Project:" path="9:$name;">
	<label name="Chose a Project:" path="/41:$name;"/>
	<list path="[2]/36:#ANY" />
  </choose>

  <open name="Open" path="9:$name;/43:$name;"/>
  <cancel name="Cancel" path="9:$name;/43:$name;"/>
  <help name="Help" path="9:$name;.../43:$name;...">
	<dialog title="FieldWorks Translation Editor Help" topic="Open a FieldWorks project" topic-path="$topic"/>
  </help>

</open-project>
